$b = $("#shu_btn");
$("#toggle").on({
	"click": function(){
		$b.toggle();
		return false;
	}
});
$(document).on({
	"click": function(e){
		var src = e.target;

		if(src.id && src.id ==="toggle"){
			return false;
		}else{
			$("#content").hide();
		}
	}
});

