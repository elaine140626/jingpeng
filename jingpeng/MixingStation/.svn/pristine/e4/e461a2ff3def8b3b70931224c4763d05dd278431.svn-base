                        <script type="text/javascript">
                        $(function() {
                            $("#toggle").click(function() {
                                $(this).text($("#content").is(":hidden") ? "拌合站" : "拌合站");
                                $("#content").slideToggle();
                            });
                        });
                        </script>
                            <span class="shu_ban" id="toggle">拌合站</span>
                            <div class="dtree" id="content" style="display:none;">
                                <script type="text/javascript">
                                var baseUrl= getContextPath();
                                var orgid = "";
                                var orgname = "";
        						$.ajax({
        							type : "POST",
        							url : baseUrl + "/Common/getOrgTree.action",
        							async:false,
        							data : {"mixStation_Type":"0"},
        							dataType : "json",
        							success : function(data) {
        								console.log(data);
        								var flag = true;        								
        								d = new dTree('d', true);
        								for(var i = 0; i < data.length; i++) {
        									if(data[i].str_org_Type == "1" || data[i].str_org_Type == 1) {
        										d.add(data[i].i_id, data[i].i_parent_Id, data[i].str_org_Name, true, false);
        										
        									} else {
        										d.add(data[i].i_id, data[i].i_parent_Id, 'authority', data[i].Id, data[i].str_org_Name ,false, false);
        										if(flag){
        											orgid = data[i].i_id;
        	        								orgname = data[i].str_org_Name;
        											//默认指向列表中第一个
        											$("#locationText").text(data[i].str_org_Name);
        											flag = false;
        										}
        									}
        								}
        								document.write(d);
        			                    d.openAll();
        							}
        						}); 
                                </script>
