        							async:false,
        							data : {"mixStation_Type":"0"},
        							dataType : "json",
        							success : function(data) {
        								var flag = true;
        								
        								d = new dTree('d', true);
        								for(var i = 0; i < data.length; i++) {
        									if(data[i].str_org_Type == "1" || data[i].str_org_Type == 1) {
        										d.add(data[i].i_id, data[i].i_parent_Id, data[i].str_org_Name, true, false);
