                      </tr>
                      <tr>
                        <td align="right">姓名：</td>
                        <td><input type="text" name="textfield2" id="textfield2" placeholder="请输入" class="zhuce_input"></td>
                      </tr>
                      
                      <tr>
                        <td align="right">权限：</td>
                        <td>
                        <div class="dtree" id="dtree_div">
						<script type="text/javascript">
                            d = new dTree('d', true);   //参数一: 树名称。参数二：单选多选 true多选  false单选  默认单选               
                            d.add(0, -1, '试验室');
                            d.add(1, 0, 'authority', '0001', '一工区', true, false);
                            d.add(4, 1, 'authority', '0004', '中心试验室1', false, false);
                            d.add(5, 1, 'authority', '0005', '中心试验室2', false, false);
                
                            // dTree实例属性以此为：  节点ID，父类ID，chechbox的名称，chechbox的值，chechbox的显示名称，
                            //chechbox是否被选中--默认是不选，chechbox是否可用：默认是可用，节点链接：默认是虚链接
                            d.add(6, 0, 'authority', '0012', '二工区 ', true, false);
                            d.add(7, 6, 'authority', '0014', '中心试验室1', false, false);
                            d.add(8, 6, 'authority', '0015', '中心试验室2', false, false);
                
                
                            d.add(11, 0, 'authority', '0016', '三工区 ', true, false);
                            d.add(13, 11, 'authority', '0018', '中心试验室3', false, false);
                            d.add(14, 11, 'authority', '0019', '中心试验室4', false, false);
                            document.write(d);
                            d.openAll();
                        </script>
                        
