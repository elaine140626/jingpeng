                	<div class="shu_btn">
                        <span class="shu_ban" id="toggle">试验室</span>
                        <div class="dtree" id="content" style="display:none;">
						<script type="text/javascript">
						 var parentNode = '';
							$.ajax({
										type : "POST",
										url : "../TestCollection/getTestRoomList",
										async : false,
										dataType : "json",
										success : function(data) {
											// 显示试验室名称
											d = new dTree('d', true);
											d.add(0, -1, '吉东公路盲样试验室');
											var html = "";
											if(data != null && data.length > 0){
												for (var i = 0; i < data.length; i++) {
													// 父类节点
													if(data[i].parentNode != null && data[i].parentNode != 0){
														// 根据父类id区分子集
														if(data[i].parentNode == parentNode){
															d.add(data[i].id,
																	data[i].parentNode,
																	'children',
																	data[i].uniqueidentifier,
																	data[i].testroomname,
																	false,
																	false);
														}else{
															// 父类显示(只选中子节点)
															d.add(data[i].id,
																	0,
																	'authority',
																	data[i].uniqueidentifier,
																	data[i].testroomname,
																	false,
																	false);
														}
													}else{
														// 父类显示
														d.add(data[i].id,
																data[i].parentNode,
																'authority',
																data[i].uniqueidentifier,
																data[i].testroomname,
																false,
																false);
														// 父类节点
														parentNode = data[i].id;
													}

													 if (i == 0) {
														 html += "<span class='s'>"
																+ data[i].testroomname
																+ "</span>";
													} else {
														html += "<span class='s'>"
														    +'、'+ data[i].testroomname
															+ "</span>";
													}

												}
											}
											$("#station").html(html);
											document.write(d);
											d.openAll();
										}
									});

							// 设置checkBox全部选中
							var authority = document.getElementsByName("authority");
							for (var i = 0; i < authority.length; i++) {
								var aut = authority[i]
								$("#" + aut.id).attr("checked",
										"checked");
							}
							var children = document.getElementsByName("children");
							for (var i = 0; i < children.length; i++) {
								var aut = children[i]
								$("#" + aut.id).attr("checked",
										"checked");
							}
                        </script>
