			<script type="text/javascript">
				//拌合站数据汇展示图表
				var myChart111 = echarts.init(document
						.getElementById('chart111'), 'chalk');
				// 指定图表的配置项和数据
				option = {
					tooltip : {},
					dataset : {
						dimensions : [ 'product', '总盘数', '合格盘数' ],
						source : [ {
							product : '拌合站1',
							'总盘数' : 500,
							'合格盘数' : 200,
							
						}, {
							product : '拌合站2',
							'总盘数' : 150,
