						}, {
							product : '拌合站4',
							'总盘数' : 366,
							'合格盘数' : 234,
							
						}]

					},
					xAxis: {
						type: 'category',
					},
					yAxis: {},
					series: [{
							type: 'bar',
						},
						{
							type: 'bar'
						}
					]
				};
				myChart111.setOption(option);
			</script>
