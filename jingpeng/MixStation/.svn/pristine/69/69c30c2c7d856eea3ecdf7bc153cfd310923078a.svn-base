                        <td>
                        <div class="dtree" id="dtree_div">
						 <script type="text/javascript">
							
							 var baseUrl= getContextPath();
							 $.ajax({
									type : "POST",
									url : baseUrl + "/Register/getPlatformOrg.html",
									async:false,
									data : {},
									dataType : "json",
									success : function(data) {
										d = new dTree('d', true);
										for(var i = 0; i < data.length; i++) {
											var org_Name = data[i].str_org_Name;
											if(data[i].str_org_Type == "0" || data[i].str_org_Type == 0) {
												d.add(data[i].i_id, -1, org_Name);
											} else if(data[i].str_org_Type == "1" || data[i].str_org_Type == 1) {
												d.add(data[i].i_id, data[i].i_parent_Id, 'authority', data[i].id, org_Name, true, false);
											} else {
												d.add(data[i].i_id, data[i].i_parent_Id, 'authority', data[i].id, org_Name ,false, false);
											}
										}
										document.write(d);
				                        d.openAll();
									}
							});  
                        </script>
                        </div>
                        </td>
