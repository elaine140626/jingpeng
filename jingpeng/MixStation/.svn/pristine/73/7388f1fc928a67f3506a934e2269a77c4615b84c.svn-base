/*树形结构样式*/
.dtree {
	white-space: nowrap;
    width:300px;
	position:absolute;
	top:49px;
	left:0;
	z-index:1000;
	background-color:rgba(255,255,255,0.9);
	border:1px solid #d3d3d3;
	border-top:0;
	border-bottom-right-radius:3px;
	border-bottom-left-radius:3px;
	font-size:14px;
/*	display:none;*/
}
.dtree img {
	border: 0px;
	vertical-align: middle;
}
.dtree a {
	color: #333;
	text-decoration: none;
}
.dtree a.node, .dtree a.nodeSel {
	white-space: nowrap;
	padding: 1px 2px 1px 2px;
}
.dtree a.node:hover, .dtree a.nodeSel:hover {
	color: #333;
	text-decoration: underline;
}
.dtree a.nodeSel {
	background-color: #c0d2ec;
}
.dtree .clip {
	overflow: hidden;
}
.rootNode {
	font-family:"微软雅黑";
	color: #666;

}
.dtree_Tree {
	padding:10px 20px 10px;
/*    width:300px;*/
	font-family:"微软雅黑";
    overflow-y:auto;   
}

.dTreeNode{
	padding:5px 0;
}
#ulSelected{
	padding:0;
}

.location .banhez{
    padding: 0 5px;
    color: #337ab7;
    font-weight: bold;
}
