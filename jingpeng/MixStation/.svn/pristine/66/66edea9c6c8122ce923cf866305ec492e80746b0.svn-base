	<select id="getPosition" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT
		  t.Qualification
		  , STUFF( 
		    ( 
		      SELECT
		        '*' + Position + ',' + Symbol + ',' + CAST(JudgmentBasis as varchar (8000)) 
		      FROM
		        JudgingBasisTest02 a 
		      WHERE
		        t.Qualification = a.Qualification FOR XML PATH ('')
		    ) 
		    , 1
		    , 1
		    , ''
		  ) children 
		FROM
		  JudgingBasisTest02 t 
		WHERE
		  IsValidData = 1 
		  and t.Aggr_Type = #{Aggr_Type}
		group by
		  Qualification
	</select>
