		  , t01.Sub_Sieve_Resi1                           -- 分计筛余（%）
		  , t01.Total_Sieve_Resi1                         -- 累计筛余（%）
		  , t01.Pass_Rate1                                -- 通过百分率（%）
		  , t01.Set_Max_Pass_Rate                         -- 上限
