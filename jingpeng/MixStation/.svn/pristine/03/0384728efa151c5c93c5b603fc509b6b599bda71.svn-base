		</foreach>
		group by v.Org_ID,v.Org_Name,CONVERT(varchar(100), v.Collect_Time, 23)



	</select>


	<select id="getmaterial_consumption" resultType="androDTO"
		parameterType="java.util.HashMap">

		select o.Org_Name,o.Id,a.*
		from Organization_Info o left
		join
		(select
		v.Org_ID
		,v.Material,SUM(v.MaterialConsumption) as
		MaterialConsumption
		from V_MaterialConsumption v left join
		Equipment_Info e on e.Id =
		v.Equ_ID left join
		Organization_Info o on
		o.Id = v.Org_ID
		where v.Org_ID
		= o.Id
		group by v.Org_ID,v.Material ) a
		on o.Id = a.Org_ID
		where
		o.Org_Type = 2 and o.MixStation_Type = '0'
		<foreach collection="org_Ids" index="index" item="item"
			open=" AND o.Id in (" separator="," close=")">
			#{item}
		</foreach>


	</select>

	<select id="getmaterial_consumption1" resultType="androDTO"
		parameterType="java.util.HashMap">

		select o.Org_Name Org_Name,v.Org_ID,CONVERT(varchar(100),
		v.Collect_Time,
		23) as
		date,v.Material,SUM(v.MaterialConsumption) as
		MaterialConsumption
		from V_MaterialConsumption v
		LEFT JOIN
		Organization_Info o on o.Id = v.Org_ID
		where 1=1
		<foreach collection="org_Ids" index="index" item="item"
			open=" AND v.Org_ID  in (" separator="," close=")">
			#{item}
		</foreach>

		group by v.Material,v.Org_ID,CONVERT(varchar(100),
		v.Collect_Time,
		23),o.Org_Name
		order by v.Material,CONVERT(varchar(100),
		v.Collect_Time, 23)


	</select>

