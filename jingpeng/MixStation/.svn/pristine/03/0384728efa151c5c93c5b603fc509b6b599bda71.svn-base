<mapper namespace="andro">

	<select id="getAsphalt_warning" resultType="androDTO"
		parameterType="java.util.HashMap">
		<!-- select o.Org_Name,o.Id,a.* from Organization_Info o left join (select 
			v.Org_ID,count(*) as TotalCount ,(select COUNT(*) from V_Asphalt_ProductionData 
			d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 and d.Analysis_Result != 
			'1' and d.Analysis_Result is not null) as Unqualified ,(select COUNT(*) from 
			V_Asphalt_ProductionData d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 
			and d.Analysis_Result like '%2%') as GLUnqualified ,(select COUNT(*) from 
			V_Asphalt_ProductionData d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 
			and d.Analysis_Result like '%3%' ) as FLUnqualified ,(select COUNT(*) from 
			V_Asphalt_ProductionData d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 
			and d.Analysis_Result like '%4%' ) as LQUnqualified ,(select COUNT(*) from 
			V_Asphalt_ProductionData d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 
			and d.Analysis_Result like '%5%' ) as WCJUnqualified ,(select COUNT(*) from 
			V_Asphalt_ProductionData d where d.Org_ID = v.Org_ID and d.Valid_Flag = 1 
			and d.Analysis_Result like '%10%' ) as JPUnqualified from V_Asphalt_ProductionData 
			v where 1=1 <foreach collection="org_Ids" index="index" item="item" open=" 
			AND v.Org_ID in (" separator="," close=")"> #{item} </foreach> and v.Valid_Flag 
			= 1 group by v.Org_ID) as a on o.Id = a.Org_ID where o.Org_Type = 2 and o.MixStation_Type 
			= '0' <foreach collection="org_Ids" index="index" item="item" open=" AND 
			o.Id in (" separator="," close=")"> #{item} </foreach> -->

		select o.Org_Name,o.Id,a.*
		from Organization_Info o left join
		(select
		v.Org_ID,v.Equipment_Name,count(*) as TotalCount
		,(select COUNT(*)
		from
		V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Equ_ID =
		v.Equ_ID and d.Valid_Flag =
		1 and d.Analysis_Result != '1'
		and
		d.Analysis_Result is not null

		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as Unqualified
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where
		d.Org_ID = v.Org_ID and d.Equ_ID = v.Equ_ID and d.Valid_Flag = 1 and
		d.Analysis_Result like
		'%2%'
		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as GLUnqualified
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Equ_ID = v.Equ_ID and d.Valid_Flag = 1
		and d.Analysis_Result
		like
		'%3%'

		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as FLUnqualified
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Equ_ID = v.Equ_ID and d.Valid_Flag = 1
		and d.Analysis_Result
		like
		'%4%'
		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as LQUnqualified
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Equ_ID = v.Equ_ID and d.Valid_Flag = 1
		and d.Analysis_Result
		like
		'%5%'
		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as WCJUnqualified
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Equ_ID = v.Equ_ID and d.Valid_Flag = 1
		and d.Analysis_Result
		like
		'%10%'
		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		) as JPUnqualified
		from V_Asphalt_ProductionData v
		where 1=1

		<foreach collection="org_Ids" index="index" item="item"
			open=" AND v.Org_ID in (" separator="," close=")">
			#{item}
		</foreach>
		and v.Valid_Flag = 1
		<if test="start_Time != null and start_Time != ''">
			AND d.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND d.collect_Time &lt;= #{end_Time}
		</if>
		group by v.Org_ID,v.Equ_ID,v.Equipment_Name ) as a on o.Id = a.Org_ID
		where o.Org_Type = 2 and
		o.MixStation_Type = '0'

		<foreach collection="org_Ids" index="index" item="item"
			open=" AND o.Id in (" separator="," close=")">
			#{item}
		</foreach>
	</select>

	<select id="getAsphalt_production_statisticslist" resultType="androDTO"
		parameterType="java.util.HashMap">
		select o.Org_Name,o.Id,a.*
		from Organization_Info o left join
		(select
		v.Org_ID,SUM(v.Total_Weight) as
		ZongChanLiang,
		SUM(v.Asphalt_Meter) as
		LQYongLiang,
		COUNT(*) as TotalCount,
		(select COUNT(*)
		from
		V_Asphalt_ProductionData a
		where 1=1
		<if test="start_Time != null and start_Time != ''">
			AND a.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND a.collect_Time &lt;= #{end_Time}
		</if>
		and a.Valid_Flag =1 and a.Analysis_Result = '1' and a.Org_ID =
		v.Org_ID ) as HeGePanShu,
		(select COUNT(*)
		from V_Asphalt_ProductionData
		a
		where 1=1

		<if test="start_Time != null and start_Time != ''">
			AND a.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND a.collect_Time &lt;= #{end_Time}
		</if>
		and a.Valid_Flag =1 and a.Analysis_Result != '1' and a.Analysis_Result
		is not null
		and a.Org_ID = v.Org_ID) as BuHeGePanShu
		from
		V_Asphalt_ProductionData v
		where 1=1
		<if test="start_Time != null and start_Time != ''">
			AND v.collect_Time &gt;= #{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND v.collect_Time &lt;= #{end_Time}
		</if>

		and v.Org_ID in(3)
		group by v.Org_ID)as a on o.Id = a.Org_ID
		where
		o.Org_Type = 2 and o.MixStation_Type = '0'
		<foreach collection="org_Ids" index="index" item="item"
			open=" AND o.Id in (" separator="," close=")">
			#{item}
		</foreach>



	</select>

	<select id="getAsphalt_production_statisticspage" resultType="androDTO"
		parameterType="androDTO">
		select CONVERT(varchar(100), v.Collect_Time, 23) Collect_Time
		,v.Org_ID,v.Org_Name,v.str_productInfo, CONVERT(varchar(100),
		v.Collect_Time, 23) as Day
		,SUM(v.Total_Weight) as
		Total_Weight,SUM(v.Asphalt_Meter) as Total_Asphalt
		,COUNT(*) as
		TotalCount
		,(select COUNT(*)
		from V_Asphalt_ProductionData d
		where
		d.Org_ID = v.Org_ID and d.Valid_Flag = 1 and d.Analysis_Result = '1'
		and CONVERT(varchar(100), d.Collect_Time, 23) = CONVERT(varchar(100),
		v.Collect_Time, 23) ) as HeGePanShu
		,(select COUNT(*)
		from
		V_Asphalt_ProductionData d
		where d.Org_ID = v.Org_ID and d.Valid_Flag =
		1 and d.Analysis_Result != '1'
		and d.Analysis_Result is not null
		and
		CONVERT(varchar(100), d.Collect_Time, 23) = CONVERT(varchar(100),
		v.Collect_Time, 23) ) as Unqualified
		from V_Asphalt_ProductionData v
		WHERE v.Org_ID =3
		<if test="start_Time != null and start_Time != ''">
			AND CONVERT(varchar(100), v.Collect_Time, 23) &gt;=
			#{start_Time}
		</if>
		<if test="end_Time != null and end_Time != ''">
			AND CONVERT(varchar(100), v.Collect_Time, 23) &lt;=
			#{end_Time}
		</if>

		group by v.Org_ID,v.Org_Name,CONVERT(varchar(100), v.Collect_Time,
		23),v.Product_ID,v.str_productInfo
	</select>
	<select id="getAsphalt_production_statisticsechar" resultType="androDTO"
