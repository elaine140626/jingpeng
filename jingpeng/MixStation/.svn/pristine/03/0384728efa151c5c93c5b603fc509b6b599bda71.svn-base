	<select id="getDeviation_Asphalt" resultType="androDTO"
		parameterType="java.util.HashMap">
		SELECT o.Org_Name,da.Id,da.Dev_Aggregate ,da.Dev_Powder
		,da.Dev_Admixture,da.Dev_Asphalt,da.MixTemperatureUp,da.MixTemperatureDown
		from Deviation_Asphalt da
		LEFT JOIN Organization_Info o on o.Id =
		da.Org_ID

		WHERE da.Valid_Flag =1
		<foreach collection="org_Ids" index="index" item="item"
			open=" AND da.Org_id in (" separator="," close=")">
			#{item}
		</foreach>

	</select>
	<insert id="addDeviation_Asphalt" parameterType="androDTO">


		INSERT INTO
		Deviation_Asphalt
		(Org_ID,Dev_Aggregate,Dev_Powder,Dev_Admixture,Dev_Asphalt,Operator,Create_Date,Valid_Flag,Upload,MixTemperatureUp,MixTemperatureDown)
		VALUES
		(#{org_id},#{Dev_Aggregate},#{Dev_Powder},#{Dev_Admixture},#{Dev_Asphalt},#{cjr},#{Create_Date},1,1,#{MixTemperatureUp},#{MixTemperatureDown})

	</insert>


	<select id="getDeviation_Cement" resultType="androDTO"
		parameterType="java.util.Map">
		SELECT
		o.Org_Name Org_Name,
		c.Org_ID Org_ID,
		c.Aggregate_Deviation Aggregate_Deviation,
		c.Cement_Deviation
		Cement_Deviation,
		c.Water_Deviation Water_Deviation,
		c.Admixture_Deviation Admixture_Deviation,
		c.Mixing_Time Mixing_Time,
		c.Effective_Time Effective_Time,
		c.Operator Operator,
		c.Create_Date
		Create_Date
		FROM
		Cement_Deviation c
		LEFT JOIN Organization_Info o ON o.Id
		= c.Org_ID
		WHERE
		c.Valid_Flag = 1
		
		<foreach collection="org_Ids" index="index" item="item"
			open=" AND c.Org_id in (" separator="," close=")">
			#{item}
		</foreach>

	</select>
	<insert id="addDeviation_Cement" parameterType="androDTO">


		INSERT INTO
		Cement_Deviation
		(Org_ID,Aggregate_Deviation,Cement_Deviation,Water_Deviation,Admixture_Deviation,Operator,Create_Date,Valid_Flag,Upload)
		VALUES
		(#{org_id},#{Aggregate_Deviation},#{Cement_Deviation},#{Water_Deviation},#{Admixture_Deviation},#{cjr},#{Create_Date},1,1)

	</insert>

