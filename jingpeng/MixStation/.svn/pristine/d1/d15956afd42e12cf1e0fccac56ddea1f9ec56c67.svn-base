.last {
	background: url(../Jqimg/camera.png) no-repeat 15% center;
	background-size: 100px 100px;
}

.cameraText {
	width: 60%;
	float: right;
	height: 100%;
	font-size: 36px;
	color: #fff;
}

#main {
	width: 60%;
	height: 85%;
	padding: 30px 15px;
	float: left;
}

#count {
	float: left;
	width: 40%;
	height: 100%;
}

#count>div {
	width: 100%;
	color: #fff;
	padding: 10px 0 0 0;
}

#count>div ul {
	list-style: none;
}

#count>div ul li {
	text-indent: 2em;
	font-size: 16px;
}

#count>div ul li span {
	font-weight: bold;
	font-size: 18px;
}

