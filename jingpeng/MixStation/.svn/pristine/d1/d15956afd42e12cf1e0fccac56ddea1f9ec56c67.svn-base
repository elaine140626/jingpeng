body {
	font-family: 'Lato', sans-serif;
	font-weight: normal;
	color: #6E6E6E;
	font-size: 15px;
	background: #fff;
	height: 100%;
	background: url(../Jqimg/bg.png) no-repeat center center;
	background-size: 100%;
	overflow: hidden;
}

html {
	height: 100%;
}

/*General*/
h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
	font-family: 'Oswald', sans-serif;
}

p {
	line-height: 28px;
}

h1, .h1, h2, .h2, h3, .h3 {
	/*margin-top: 30px;*/
	color: rgb(41, 51, 103);
}

h2 {
	margin: 30px 0;
}

a {
	/*color: #0DC0C0;*/
	color: #fff;
}

a:hover, a:focus {
	color: #00BBFF;
	text-decoration: none;
}

#contain {
	width: 99%;
	height: 98%;
	margin: 0.5%;
}

.half {
	width: 49%;
	height: 49%;
	float: left;
	overflow: hidden;
	background: rgba(11, 44, 248, 0.2);
	border: 1px solid #0639CB;
	border-radius: 1px;
	-webkit-box-shadow: 0 0 20px #0cc;
	-moz-box-shadow: 0 0 20px #0cc;
	box-shadow: 0 0 20px #0cc;
}

.mleft0 {
	margin-left: 0.5%;
}

.mleft1 {
	margin-left: 1%;
}

.mbottom {
	margin-bottom: 1%;
}

.half .title {
	width: 100%;
	text-indent: 1em;
	margin-top: 0;
	color: #5160bd;
	padding: 12px;
	height: 56px;
	font-size: 30px;
	margin: 0;
	color: rgba(255, 255, 255, .7);
	background: #293567;
}

#mp4 {
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.cameraDetai0 {
	width: 50%;
	height: 100%;
	overflow: hidden;
	float: left;
}

