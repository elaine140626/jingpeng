<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="VideoSurveillance">

	<select id="getVideoSurveillance" parameterType="java.util.Map" resultType="VideoSurveillance">
	
		select * from VideoSurveillance
			
	</select>
	
	
	<select id="getVideoSurveillanceById" parameterType="java.util.Map" resultType="VideoSurveillance">
	
		select * from VideoSurveillance where vid =  #{vid}
		
	</select>
	
</mapper>